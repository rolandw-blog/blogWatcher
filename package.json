{
	"name": "myproject",
	"author": "Roland Warburton",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "npm run build && node --require 'dotenv/config' --inspect dist/index.js",
		"dev": "DEBUG=app* nodemon --config nodemon.json src/index.ts",
		"build": "tsc -p .",
		"prettier:watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"lint:watch": "onchange 'src/**/*.ts' -- npm run lint",
		"test": "jest --config=jest.config.js",
		"test:unit": "SUITE=unit jest --config=jest.config.js --watch",
		"test:intergration": "SUITE=intergration jest --config=jest.config.js  --watchAll",
		"swagger": "tsc -p openapi && node dist/swagger/index.js"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"keywords": [],
	"license": "ISC",
	"dependencies": {
		"ajv": "^7.1.1",
		"cors": "^2.8.5",
		"debug": "^3.2.7",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"helmet": "^4.6.0",
		"is-docker": "^2.2.1",
		"mongoose": "^5.13.3",
		"winston-transport": "^4.4.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.10",
		"@types/debug": "^4.1.5",
		"@types/express": "^4.17.12",
		"@types/jest": "^26.0.23",
		"@types/node": "^15.12.2",
		"@types/supertest": "^2.0.11",
		"@types/swagger-jsdoc": "^6.0.0",
		"@types/swagger-ui-express": "^4.1.2",
		"@typescript-eslint/eslint-plugin": "^4.26.1",
		"@typescript-eslint/parser": "^4.28.1",
		"concurrently": "^6.2.0",
		"eslint": "^7.29.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-jest": "^24.3.6",
		"eslint-plugin-node": "^11.1.0",
		"jest": "^27.0.6",
		"nodemon": "^2.0.7",
		"onchange": "^7.1.0",
		"prettier": "^2.3.1",
		"supertest": "^6.1.3",
		"swagger-jsdoc": "^6.1.0",
		"ts-jest": "^27.0.3",
		"ts-node": "^10.0.0",
		"typescript": "^4.3.2",
		"winston": "^3.3.3",
		"winston-daily-rotate-file": "^4.5.5"
	}
}
